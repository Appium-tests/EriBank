<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="Login view">
    
    <listeners>
        <listener class-name="qa.listeners.Listener"/>
    </listeners>

    <parameter name="fileName" value="EB_Credentials.json"/>

    <test name="Login form input text verification">
        <classes>
            <class name="login.InputTextVerificationTest"/>
        </classes>
    </test>

    <test name="Login to an account">
        <classes>
            <class name="login.LoginTest"/>
        </classes>
    </test>
    
</suite>